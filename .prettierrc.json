{
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 120,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"trailingComma": "all",
	"endOfLine": "auto",
	"semi": false,
	"arrowParens": "always",
	"bracketSameLine": true,
	"overrides": [
		{
			"files": "**/*.html",
			"options": {
				"singleQuote": true,
				"jsxSingleQuote": true
			}
		}
	]
}
